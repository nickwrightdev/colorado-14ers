{
  "name": "colorado-14ers",
  "version": "0.0.0-semantically-released",
  "description": "Get random Colorado 14ers",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "istanbul cover -x src/*.test.js _mocha -- -R spec src/index.test.js",
    "test:watch": "mocha src/index.test.js -w",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nickwrightdev/colorado-14ers.git"
  },
  "keywords": [
    "random",
    "colorado",
    "14ers"
  ],
  "author": "Nick Wright <nick@nickwrightdev.com> (http://nickwrightdev.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickwrightdev/colorado-14ers/issues"
  },
  "homepage": "https://github.com/nickwrightdev/colorado-14ers#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.8.6",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "1.3.2",
    "istanbul": "0.4.5",
    "mocha": "3.1.2",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  }
}
