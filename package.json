{
  "name": "colorado-14ers",
  "version": "0.0.0-semantically-released",
  "description": "Get random Colorado 14ers",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha src/index.test.js --compilers js:babel-register",
    "test:watch": "npm t -- -w",
    "cover": "nyc npm t",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nickwrightdev/colorado-14ers.git"
  },
  "keywords": [
    "random",
    "colorado",
    "14ers"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Nick Wright <nick@nickwrightdev.com> (http://nickwrightdev.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickwrightdev/colorado-14ers/issues"
  },
  "homepage": "https://github.com/nickwrightdev/colorado-14ers#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-2": "6.17.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.8.6",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "1.3.2",
    "istanbul": "0.4.5",
    "mocha": "3.1.2",
    "nyc": "8.3.2",
    "rimraf": "2.5.4",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
